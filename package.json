{
  "name": "demo2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config scripts/start.js",
    "build": "webpack --config scripts/build.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "import",
        {
          "libraryName": "antd",
          "libraryDirectory": "es",
          "style": "css"
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "./scripts/config/eslintrc.js"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "feat",
          "fix",
          "docs",
          "style",
          "refactor",
          "perf",
          "test",
          "build",
          "ci",
          "chore",
          "revert"
        ]
      ],
      "type-case": [
        2,
        "always",
        [
          "lower-case"
        ]
      ],
      "type-empty": [
        2,
        "never"
      ],
      "scope-empty": [
        2,
        "never"
      ],
      "scope-case": [
        2,
        "always",
        [
          "lower-case",
          "upper-case",
          "camel-case",
          "kebab-case",
          "pascal-case",
          "sentence-case",
          "snake-case",
          "start-case"
        ]
      ],
      "subject-case": [
        2,
        "always",
        [
          "lower-case",
          "upper-case",
          "camel-case",
          "kebab-case",
          "pascal-case",
          "sentence-case",
          "snake-case",
          "start-case"
        ]
      ],
      "subject-empty": [
        2,
        "never"
      ],
      "subject-full-stop": [
        2,
        "never"
      ],
      "header-max-length": [
        2,
        "always",
        72
      ]
    }
  },
  "lint-staged": {
    "{src,static}/**/*.{js,jsx,mjs}": [
      "node_modules/.bin/eslint --fix",
      "node_modules/.bin/prettier --write",
      "git add"
    ],
    "{src,static}/**/*.{ts,tsx}": [
      "node_modules/.bin/tslint --fix",
      "node_modules/.bin/prettier --write",
      "git add"
    ],
    "{src,static}/**/*.{css,scss,less,json}": [
      "node_modules/.bin/prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/node": "^11.11.0",
    "@types/react": "^16.8.7",
    "@types/react-dom": "^16.8.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "clean-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.15.0",
    "eslint-config-react-app": "^3.0.7",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin-alt": "^0.4.14",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-notifier": "^5.4.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "raw-loader": "^1.0.0",
    "react-dev-utils": "^7.0.3",
    "resolve": "^1.10.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.3333",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "antd": "^3.15.2",
    "axios": "^0.18.0",
    "lint-staged": "^8.1.5",
    "mobx": "^5.9.0",
    "mobx-react": "^5.4.3",
    "moment": "^2.24.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-router-dom": "^4.3.1"
  }
}
